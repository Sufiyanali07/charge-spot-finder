{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nexport default {\n  name: 'App',\n  computed: {\n    ...mapGetters(['isLoggedIn', 'user'])\n  },\n  methods: {\n    ...mapActions(['logout'])\n  },\n  mounted() {\n    // Ensure Bootstrap's JavaScript is properly initialized\n    // This will make dropdowns, modals, and other interactive elements work\n    import('bootstrap/dist/js/bootstrap.bundle.min.js').then(() => {\n      console.log('Bootstrap JS initialized successfully');\n    }).catch(error => {\n      console.error('Failed to initialize Bootstrap JS:', error);\n    });\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","name","computed","methods","mounted","then","console","log","catch","error"],"sources":["C:\\Users\\admin\\OneDrive\\Desktop\\charge-spot-finder\\frontend\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <nav class=\"navbar navbar-expand-lg navbar-dark bg-primary\">\n      <div class=\"container\">\n        <router-link class=\"navbar-brand\" to=\"/\">EV Charging Station</router-link>\n        <button class=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" data-bs-target=\"#navbarNav\">\n          <span class=\"navbar-toggler-icon\"></span>\n        </button>\n        <div class=\"collapse navbar-collapse\" id=\"navbarNav\">\n          <ul class=\"navbar-nav me-auto\">\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" to=\"/\">Home</router-link>\n            </li>\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" to=\"/stations\">Stations</router-link>\n            </li>\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" to=\"/map\">Map View</router-link>\n            </li>\n          </ul>\n          <ul class=\"navbar-nav\" v-if=\"!isLoggedIn\">\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" to=\"/login\">Login</router-link>\n            </li>\n            <li class=\"nav-item\">\n              <router-link class=\"nav-link\" to=\"/register\">Register</router-link>\n            </li>\n          </ul>\n          <ul class=\"navbar-nav\" v-else>\n            <li class=\"nav-item dropdown\">\n              <a class=\"nav-link dropdown-toggle\" href=\"#\" role=\"button\" data-bs-toggle=\"dropdown\">\n                {{ user.username }}\n              </a>\n              <ul class=\"dropdown-menu dropdown-menu-end\">\n                <li><router-link class=\"dropdown-item\" to=\"/profile\">Profile</router-link></li>\n                <li><hr class=\"dropdown-divider\"></li>\n                <li><a class=\"dropdown-item\" href=\"#\" @click.prevent=\"logout\">Logout</a></li>\n              </ul>\n            </li>\n          </ul>\n        </div>\n      </div>\n    </nav>\n\n    <main class=\"container py-4\">\n      <router-view />\n    </main>\n\n    <footer class=\"footer mt-auto py-3 bg-light\">\n      <div class=\"container text-center\">\n        <span class=\"text-muted\">EV Charging Station Management System &copy; {{ new Date().getFullYear() }}</span>\n      </div>\n    </footer>\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions } from 'vuex';\n\nexport default {\n  name: 'App',\n  computed: {\n    ...mapGetters(['isLoggedIn', 'user'])\n  },\n  methods: {\n    ...mapActions(['logout'])\n  },\n  mounted() {\n    // Ensure Bootstrap's JavaScript is properly initialized\n    // This will make dropdowns, modals, and other interactive elements work\n    import('bootstrap/dist/js/bootstrap.bundle.min.js')\n      .then(() => {\n        console.log('Bootstrap JS initialized successfully');\n      })\n      .catch(error => {\n        console.error('Failed to initialize Bootstrap JS:', error);\n      });\n  }\n}\n</script>\n\n<style>\n@import 'https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css';\n\n#app {\n  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  min-height: 100vh;\n  display: flex;\n  flex-direction: column;\n}\n\nmain {\n  flex: 1;\n}\n\n.navbar-brand {\n  font-weight: bold;\n}\n\n.footer {\n  margin-top: auto;\n}\n\n/* Use forced-colors media query instead of -ms-high-contrast */\n@media (forced-colors: active) {\n  .btn-primary {\n    background-color: Highlight;\n    border-color: Highlight;\n    color: HighlightText;\n  }\n  \n  .btn-secondary {\n    background-color: ButtonFace;\n    border-color: ButtonText;\n    color: ButtonText;\n  }\n  \n  .btn-danger {\n    background-color: Highlight;\n    border-color: Highlight;\n    color: HighlightText;\n  }\n  \n  .navbar-dark {\n    background-color: Highlight !important;\n    color: HighlightText !important;\n  }\n  \n  .card {\n    border-color: ButtonText;\n  }\n}\n</style>\n"],"mappings":"AAyDA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,QAAQ,EAAE;IACR,GAAGH,UAAU,CAAC,CAAC,YAAY,EAAE,MAAM,CAAC;EACtC,CAAC;EACDI,OAAO,EAAE;IACP,GAAGH,UAAU,CAAC,CAAC,QAAQ,CAAC;EAC1B,CAAC;EACDI,OAAOA,CAAA,EAAG;IACR;IACA;IACA,MAAM,CAAC,2CAA2C,EAC/CC,IAAI,CAAC,MAAM;MACVC,OAAO,CAACC,GAAG,CAAC,uCAAuC,CAAC;IACtD,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;MACdH,OAAO,CAACG,KAAK,CAAC,oCAAoC,EAAEA,KAAK,CAAC;IAC5D,CAAC,CAAC;EACN;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}